{
  "version": 2,
  "functions": {
    "api": {
      "source": "./src/server.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "function": "api"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}